RewriteEngine On
RewriteBase /



	ReWriteRule ^jquery$ http://ajax.googleapis.com/ajax/libs/jquery/1.5.2/jquery.min.js [P]

	RewriteCond %{HTTP_HOST} ^www\.(.+)$ [NC]
	RewriteRule ^(.*)$ http://%1/$1 [R=301,L]
